i-bookshelf\" : \"mdi-book-open-variant\");\n      });\n      this.bookLabels = allLabels;\n    },\n    iconTitle(id) {\n      return this.openAt(id) ? \"Close book\":\"Open book\";\n    },\n    routeLink(item) {\n      return '/read/'+item._id+'/1';\n    }\n  },\n  computed: {},\n  meteor: {},\n};\n"]},"sourceType":"module","hash":"5d32b2f686fc8568805e718fca012831aaccf8f0"}
